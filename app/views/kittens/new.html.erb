<div class ="container">
  <div>
    <h1> Create a new kitten</h1>
  </div>
  <%=form_with model:@kitten do |f| %>
    <div>
      <%=f.label :name, "Name" ,:class =>"form-label"%>
      <%=f.text_field :name , :class=>"form-control"%>
    </div>
    <div>
      <%=f.label :age,"Age",:class =>"form-label"%>
      <%=f.number_field :age , :class=>"form-control"%>
    </div>
    <div>
      <%=f.label :cuteness ,"Cuteness",:class =>"form-label"%>
      <%=f.number_field :cuteness , :class=>"form-control"%>
    </div>
    <div>
      <%=f.label :softness ,"Softness",:class =>"form-label"%>
      <%=f.number_field :softness , :class=>"form-control"%>
    </div>
    <div>
      <%=f.submit "Create",:class=>"btn btn-primary btn-lg"%>
    </div>



  <%end%>
</div>
